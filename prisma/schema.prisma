// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

enum SeverityLevel {
  LOW
  MEDIUM
  HIGH
}

model LogModel {
  id        Int           @id @default(autoincrement())
  message   String
  origin    String
  level     SeverityLevel
  createdAt DateTime      @default(now())
}
